{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "plugin:flowtype/recommended",
    "eslint-config-airbnb-base"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "flowtype"
  ],
  "rules": {
    "strict": 0,
    "semi": 0,
    "no-console": 1
  }
}
